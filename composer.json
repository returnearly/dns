{
    "name": "returnearly/dns",
    "description": "A package for resolving and normalizing DNS records.",
    "keywords": [
        "returnearly",
        "dns"
    ],
    "homepage": "https://github.com/returnearly/dns",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Schlick",
            "email": "tom@returnearly.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "badcow/dns": "^4.2",
        "returnearly/laravel-enums": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ReturnEarly\\Dns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReturnEarly\\Dns\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
